generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model students {
  id         Int      @id @default(autoincrement())
  name       String
  cohort     String
  date_joined DateTime
  last_login DateTime
  status     Boolean
}